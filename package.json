{
    "name": "discord-bot",
    "version": "1.0.0",
    "description": "Discord Bot Template",
    "engines": {
        "node": ">=14",
        "npm": ">=6.14.7",
        "yarn": ">=1.22.4 <2.0.0"
    },
    "homepage": "https://github.com/Pepijn98/discord-bot#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/Pepijn98/discord-bot.git"
    },
    "bugs": {
        "url": "https://github.com/Pepijn98/discord-bot/issues"
    },
    "author": {
        "email": "pepijn@vdbroek.dev",
        "name": "Pepijn van den Broek",
        "url": "https://vdbroek.dev"
    },
    "scripts": {
        "deploy": "yarn pm2:prod",
        "pm2:prod": "pm2 start ecosystem.config.js --env production",
        "pm2:dev": "pm2 start ecosystem.config.js --env development",
        "start": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register src/index.ts",
        "test": "yarn dev",
        "dev": "cross-env NODE_ENV=development nodemon --watch '**/*.ts' --ignore '**/*.spec.ts' --exec 'ts-node' -r tsconfig-paths/register src/index.ts",
        "prebuild": "rm -rf dist",
        "pretest": "yarn lint",
        "prestart": "yarn lint",
        "build": "tsc",
        "deps": "depcheck",
        "format": "prettier --write \"src/**/*.{ts,js}\" \"./*.{ts,js}\"",
        "lint": "eslint \"src/**/*.{ts,js}\" \"./*.{ts,js}\" --fix",
        "pretty": "yarn format && yarn lint",
        "clean": "rm -rf ./node_modules ./package-lock.json ./yarn.lock"
    },
    "dependencies": {
        "@kurozero/collection": "^0.5.1",
        "bufferutil": "^4.0.1",
        "chalk": "^4.1.0",
        "eris": "^0.13.3",
        "erlpack": "abalabahaha/erlpack",
        "moment": "^2.27.0",
        "os-utils": "oscmejia/os-utils",
        "winston": "^3.3.3",
        "zlib-sync": "^0.1.7"
    },
    "devDependencies": {
        "@types/node": "^14.6.3",
        "@typescript-eslint/eslint-plugin": "^4.0.1",
        "@typescript-eslint/parser": "^4.0.1",
        "cross-env": "^7.0.2",
        "eslint": "^7.8.1",
        "eslint-config-prettier": "^6.11.0",
        "gts": "^2.0.2",
        "nodemon": "^2.0.4",
        "ts-node": "^9.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.0.2"
    }
}
